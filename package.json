{
  "name": "thundera-lib",
  "version": "0.1.1",
  "private": false,
  "main": "dist/thundera-lib.umd.js",
  "module": "dist/thundera-lib.esm.js",
  "style": "dist/thundera-lib.css",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name ThunderaLib --dest dist ./src/main.js --extract-css",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@formkit/vue": "^1.6.9",
    "@vue-a11y/announcer": "^3.1.5",
    "@vue-a11y/skip-to": "^2.1.2",
    "core-js": "^3.8.3",
    "material-design-icons-iconfont": "^6.7.0",
    "vue": "^3.2.13",
    "vue-final-modal": "^4.5.5",
    "vue-focus-lock": "^3.0.0",
    "vue-router": "^4.0.13",
    "vue3-accessible-tabs": "^0.7.0"
  },
  "peerDependencies": {
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "axe-core": "^4.10.2",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-axe": "^3.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "exports": {
    ".": {
      "import": "./dist/thundera-lib.esm.js",
      "require": "./dist/thundera-lib.umd.js"
    },
    "./style": {
      "default": "./dist/thundera-lib.css"
    }
  }
}
